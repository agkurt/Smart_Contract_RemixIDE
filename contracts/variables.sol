pragma solidity ^0.8.7 ; 

contract Function {

    uint x = 3 ; 

    function setX (uint y ) public view returns(uint) {

        return x + y ;

}

    function add( uint a , uint b ) public view returns (uint) {

        return a + b + block.timestamp ; 





}
function add2 ( uint a , uint b ) public pure returns (uint) {

    return a+ b ;




}





}